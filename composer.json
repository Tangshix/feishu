{
    "name": "larafly/feishu",
    "description": "laravel 飞书sdk",
    "authors": [
        {
            "name": "foryoufeng",
            "email": "foryoufeng@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.9",
    },
    "require-dev": {
        "pestphp/pest": "^3.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Larafly\\Feishu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larafly\\Feishu\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larafly\\Feishu\\FeishuServiceProvider"
            ]
        }
    }
}
